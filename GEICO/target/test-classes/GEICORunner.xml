<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="GEICO Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="60.0"/>
    <parameter name="url" value="https://www.geico.com//"/>

    <test name="AutoInsurancePackage">
        <classes>
            <class name="autoinsurancepackagetest.GetQuoteTest"/>
        </classes>
    </test>
    <test name="HomeOwnersPackage">
        <classes>
            <class name="homeownerspackagetest.CustomerInformationTest"/>
        </classes>
    </test>
    <test name = "HomePage">
        <classes>
            <class name="homepage.DropDownTest"/>
            <class name="homepage.FeedbackTest"/>
            <class name="homepage.InsuranceProductsTest"/>
            <class name ="homepage.LoginTest" />
            <class name="homepage.LoginTestGoogleSheet"/>
            <class name="homepage.SearchTest"/>
        </classes>
    </test>
    <test name ="Navigation">
        <classes>
            <class name="navigationtest.InformationAndInsuranceHoverTest"/>
            <class name="navigationtest.NavigationBarTest"/>
        </classes>
    </test>
</suite>